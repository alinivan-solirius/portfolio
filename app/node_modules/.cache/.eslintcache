m.Control.Feedback>Looks good!</Form.Control.Feedback>\n                                        </Form.Group>\n                                        <Form.Group as={Col} md=\"12\" controlId=\"validationCustom02\">\n                                            <Form.Label  style={{fontWeight:\"bold\"}}>URL</Form.Label>\n                                            <Form.Control  style={{padding:\"1rem\", marginBottom:\"1rem\"}}\n                                                           required\n                                                           type=\"text\"\n                                                           value={\"URL\"}\n                                            />\n                                            <Form.Control.Feedback>Looks good!</Form.Control.Feedback>\n                                        </Form.Group>\n                                        <Form.Group as={Col} md=\"12\" controlId=\"exampleForm.ControlTextarea1\">\n                                            <Form.Label  style={{fontWeight:\"bold\"}}>Update description</Form.Label>\n                                            <Form.Control style={{padding:\"1rem\", marginBottom:\"1rem\"}}\n                                                          required\n                                                          as=\"textarea\" rows={4}\n                                                          type=\"text\"\n                                                          value={\"Description\"}\n                                            />\n                                            <Form.Control.Feedback>Looks good!</Form.Control.Feedback>\n                                        </Form.Group>\n                                    </Row>\n                                </Form>\n                            </Modal.Body>\n                            <Modal.Footer>\n                                <Button variant=\"light\" onClick={handleCloseEditModal}>Close</Button>\n     